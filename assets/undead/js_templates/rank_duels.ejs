<% 
if(typeof duels != 'undefined'){ 
if(duels != false){
%>
<div class="row">
	<div class="col-12"> 
		<div class="table-responsive-sm">
		<table class="table dmn-rankings-table table-striped">
			<thead>
			<tr>
				<th class="text-center">#</th>
				<th><%= App.lc.translate('Name').fetch() %></th>
				<th class="text-center"><%= App.lc.translate('Win').fetch() %></th>
				<th class="text-center"><%= App.lc.translate('Lose').fetch() %></th>
				<th class="text-center"><%= App.lc.translate('Ratio').fetch() %></th>
			</tr>
			<tbody>
			<% for(var i=0; i< duels.length; i++) { %>
			<tr>
				<td class="text-center"><%= i+1 %></td>
				<td><a href="<%= base_url %>character/<%= duels[i].name_hex %>/<%= server_selected %>"><%= duels[i].name %></a></td>
				<td class="text-center"><%= duels[i].win %></td>
				<td class="text-center"><%= duels[i].lose %></td>
				<td class="text-center"><%= duels[i].ratio %></td>
			</tr>  
			<% } %>
			</tbody>
		</table> 
		</div>
	</div>
</div>			
<% if(typeof cache_time != 'undefined'){ %>
	<div class="row mb-4">
		<div class="col-11"> 
			<div class="float-right"><%= cache_time %></div>
		</div>	
	</div>
<% } %>
<% 
}
else{
%>
	<div class="alert alert-primary" role="alert"><%= App.lc.translate('No Players Found').fetch() %></div>
<%
}
} 
%>